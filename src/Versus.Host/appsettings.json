{
    "Serilog": {
        "Using": [
            "Serilog.Sinks.Console",
            "Serilog.Sinks.File"
        ],
        "MinimumLevel": "Warning",
        "WriteTo": [
            {
                "Name": "Console"
            },
            {
                "Name": "File",
                "Args": {
                    "path": "Logs/log.txt",
                    "rollingInterval": "Day"
                }
            }
        ],
        "Enrich": [
            "FromLogContext"
        ]
    },
    "ConnectionStrings": {
        "Default": "Host=localhost;Database=versus;Username=postgres;Password=postgres"
    },
    "Authentication": {
        "Schemes": {
            "KeyCloak": {
                "MetadataAddress": "",
                "Authority": "",
                "Audience": ""
            },
            "Google": {
                "ClientId": "",
                "ClientSecret": ""
            },
            "Facebook": {
                "AppId": "",
                "AppSecret": ""
            }
        }
    },
    "AllowedHosts": "*"
}
