{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.File"
    ],
    "MinimumLevel": "Warning",
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "Logs/log.txt"
        }
      }
    ]
  },
  "ConnectionStrings": {
    "Default": "Host=localhost;Database=versus;Username=postgres;Password=postgres"
  },
  "AllowedHosts": "*"
}
